{
  "routes": [
    {
      "method": "GET",
      "path": "/hello",
      "handler": "Hello.index"
    },
    {
      "method": "GET",
      "path": "/hi",
      "handler": "Hello.hi",
      "config": {
        "policies": [
          "plugins::users-permissions.isAuthenticated"
        ]
      }
    },
    {
      "method": "PUT",
      "path": "/hello",
      "handler": "hello.update",
      "config": {
        "policies": []
      }
    },
    {
      "method": "DELETE",
      "path": "/hello",
      "handler": "hello.delete",
      "config": {
        "policies": []
      }
    }
  ]
}
